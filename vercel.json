{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/code/frontend/$1"
    }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "vercel-python@0.1.1",
      "memory": 1024,
      "maxDuration": 10
    }
  }
}

